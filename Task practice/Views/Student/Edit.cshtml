
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>
<form  method="post">
<input type="hidden" name="Id" value="@Model.Id" />
<input type="text" name="Name" value="@Model.Name" class="form-control" />
<br />
<input type="text" name="Email" value="@Model.Email" class="form-control" />
<br />
<input type="date" name="DateOfBirth" value="@Model.DateOfBirth.ToString("yyyy-MM-dd")" class="form-control" />
<br />
<input type="submit" accept="" class="btn btn-primary" value="Update" />
</form>

